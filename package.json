{
  "name": "@vinorcola/utils",
  "version": "1.0.1",
  "description": "Basic utils to enhance common javascript operations.",
  "keywords": [
    "utils",
    "common"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vinorcola/utils.git"
  },
  "bugs": {
    "url": "https://github.com/Vinorcola/utils/issues"
  },
  "homepage": "https://github.com/Vinorcola/utils#readme",
  "type": "module",
  "exports": {
    "./classNames": {
      "default": "./lib/ClassNames.js"
    },
    "./color": {
      "default": "./lib/Color.js"
    },
    "./list": {
      "default": "./lib/List.js"
    },
    "./number": {
      "default": "./lib/Number.js"
    },
    "./object": {
      "default": "./lib/Object.js"
    },
    "./text": {
      "default": "./lib/Text.js"
    }
  },
  "author": "Vinorcola",
  "private": false,
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A .",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@vinorcola/lint": "^1.0.5",
    "typescript": "^5.9.3"
  }
}
